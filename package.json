    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/test-utils": "^2.2.10",
    "bootstrap": "^5.3.3",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.3.0",
    "jsdom": "^24.0.0",
    "sass": "^1.58.0",
    "typescript": "^5.4.5",
    "vite": "^5.2.11",
    "vite-plugin-dts": "^3.9.1",
    "vitest": "^1.6.0"
  }
