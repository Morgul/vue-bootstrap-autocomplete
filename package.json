{
  "name": "@vue-bootstrap-components/vue-bootstrap-autocomplete",
  "version": "2.16.0-alpha.0",
  "private": false,
  "description": "A typeahead/autocomplete component for Vue 2 using Bootstrap 4",
  "keywords": [
    "vue",
    "autocomplete",
    "bootstrap",
    "bootstrap 5",
    "omnisearch",
    "typeahead"
  ],
  "homepage": "https://github.com/morgul/vue-bootstrap-autocomplete",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/morgul/vue-bootstrap-autocomplete.git"
  },
  "bugs": {
    "url": "https://github.com/morgul/vue-bootstrap-autocomplete/issues"
  },
  "license": "MIT",
  "author": "Christopher S. Case <chris.case@g33xnexus.com>",
  "files": [
    "dist",
    "src"
  ],
  "main": "./dist/vue-bootstrap-autocomplete.umd.js",
  "module": "./dist/vue-bootstrap-autocomplete.es.js",
  "exports": {
    ".": {
      "types": "./dist/lib.d.ts",
      "import": "./dist/vue-bootstrap-autocomplete.es.js",
      "require": "./dist/vue-bootstrap-autocomplete.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest watch",
    "test:no-watch": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "resize-observer-polyfill": "^1.5.1",
    "vue": "^3.1.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.8.0",
